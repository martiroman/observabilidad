apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  name: tiendapc-metrics
  namespace: test-nodejs-app
  labels:
    release: prometheus
spec:
  selector:
    matchLabels:
      app.kubernetes.io/app: tiendapc
  namespaceSelector:
    matchNames:
    - test-nodejs-app
  endpoints:
  - port: frontend